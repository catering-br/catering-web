        <script type="text/javascript">
            /*** CONFIG init and gets ***/
            var COUNTRY = null;
            var LANGUAGE = null;
            var LOCATION_FIELDS = null;
            function $getCountry(){
                if(COUNTRY) return COUNTRY;
                var FALLBACK_COUNTRY = 'BR';
                COUNTRY = $('#config-country').val().trim().replace('_','-') || FALLBACK_COUNTRY;
                return COUNTRY;
            }
            function $getLanguage(){
                if(LANGUAGE) return LANGUAGE;
                var FALLBACK_LANGUAGE = 'pt-BR';
                LANGUAGE = $('#config-language').val().trim().replace('_','-') || FALLBACK_LANGUAGE;
                return LANGUAGE;
            }
            function $getDomainConfig(config){
                return _domainConfigs[config] || '';
            }
            function $getLocationFields(){
                if(LOCATION_FIELDS) return LOCATION_FIELDS;
                var FALLBACK_LOCATION_FIELDS = '';
                LOCATION_FIELDS = $('#config-location-fields').val().trim() || FALLBACK_LOCATION_FIELDS;
                return LOCATION_FIELDS;
            }
            /*** end CONFIG ***/
            /*** I18N Init ***/
            if(!$("#hid-lang").val()){
                lang = "pt-BR";
            } else {
                lang = $("#hid-lang").val();
            }
            i18n.init({
                lng : $getLanguage(),
                detectLngQS: 'lang',
                fallbackLng: 'pt-BR',
                load: 'current',
                resGetPath: _ctx + '/' + _ctxVer + '/js/locales/messages.properties.__lng__.json',
                debug: true,
                getAsync: false
            });
            /** end I18N Init **/
        </script>
        









    

















